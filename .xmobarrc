Config { font = "xft:terminus"
       , bgColor = "black"
       , fgColor = "Aquamarine3"
       , position = Top
       , commands = [ Run Weather "LOWZ" ["-t","<station>: <tempC>Â°C, <skyCondition>","-L","18","-H","25","--normal","aquamarine3","--high","chocolate1","--low","lightgreen"] 36000 
       	 	    , Run Network "eth0" ["-L","0","-H","32","--normal","aquamarine2","--high","chocolate1"] 10
                    , Run Cpu ["-L","15","-H","50","--normal","aquamarine1","--high","chocolate1"] 10
                    , Run Memory ["-t","Mem: <usedratio>"] 10
    		    , Run Date "%a %b %_d %Y" "date" 360
		    , Run Date "%H:%M:%S" "time" 10
		    , Run StdinReader
		    , Run CpuFreq ["-t","<cpu0>Ghz"] 10
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{[%eth0%][%cpu%][%memory%]<fc=DarkSlateGray3>[ %date% ]</fc><fc=aquamarine3,black>[ %time% ]</fc>"
       }
